pipeline {
    agent any
    environment {
       REGISTRY = "registry.diamol:5000"
    }
    stages {
        stage('Verify') {
            bat 'docker version'
            bat 'docker-compose version'
        }
        stage('Build') {
            bat ./ci/01-build.bat
        }
        stage('Test') {
            bat ./ci/02-test.bat
        }
        stage('Pubat') {
            bat ./ci/03-push.bat
        }
    }
}